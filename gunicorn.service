[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/home/OnlineEXP-GLS
ExecStart=/home/OnlineEXP-GLS/venv/bin/gunicorn  --access-logfile -  --workers 3  --bind unix:/run/gunicorn.sock  OnlineEXP.wsgi:application

[Install]
WantedBy=multi-user.target
