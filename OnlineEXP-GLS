server {
    listen 80;
    server_name  www.vwsupplierqualityday2021.com  vwsupplierqualityday2021.com  161.35.239.8;
    #esto es para permitir archivos de  menos de 100 MB
    client_max_body_size 100M;

    #location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        alias /home/OnlineEXP-GLS/static/;#el nombre al final debe ir con /
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }
}
